package coneccion;

public class Coneccion {
	public static void Conn;
	private String URL;
	private String Driver;
	private String Db;
	public Coneccion(String URL, String Driver, String Db) {
		this.URL = URL;
		this.Driver = Driver;
		this.Db = Db;
	}
	private void connect() {
		try {
			Class.forName(this.Driver);
		} catch (Exception e) {
			// TODO: handle exception
		}

	}


}
